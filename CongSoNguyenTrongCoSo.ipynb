   "source": [
    "a = [1,3,2,4]\n",
    "b = [0,4,3,2]\n",
    "base = 5\n",
    "k = len(a) # =len(b)\n",
    "r = 0\n",
    "s = [0] * (k+1)\n",
    "for i in range(k):\n",
    "    t = a[i] + b[i] + r\n",
    "    s[i] = t % base\n",
    "    r = t // base \n",
    "s[k] = r\n",
    "s"
