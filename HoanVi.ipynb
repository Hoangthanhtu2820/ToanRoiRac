   "source": [
    "def permutations(n):\n",
    "    if n == 1:\n",
    "        return [[1]]\n",
    "    A = []\n",
    "    for a in permutations(n-1):\n",
    "        for i in range(n):\n",
    "            b = a.copy()\n",
    "            b.insert(i,n)\n",
    "            A.append(b)\n",
    "    return A"
   ]
